  "version": "1.0.0",
  "description": "A fresh and elegant admin template, based on Vue3、Vite3、TypeScript、NaiveUI and UnoCSS. 一个基于Vue3、Vite3、TypeScript、NaiveUI and UnoCSS的清新优雅的中后台模版。",
  "author": {
    "name": "XiaoZhang",
    "email": "2715158815@qq.com",
    "url": "https://github.com/mrzym99/vue3-naive-admin"
  },
  "license": "MIT",
  "homepage": "https://github.com/mrzym99/vue3-naive-admin",
  "repository": {
    "url": "https://github.com/mrzym99/vue3-naive-admin.git"
  },
  "bugs": {
    "url": "https://github.com/mrzym99/vue3-naive-admin/issues"
  },
  "keywords": [
    "Vue3 admin ",
    "vue-admin-template",
    "Vite5",
    "TypeScript",
    "naive-ui",
    "naive-ui-admin",
    "ant-design-vue v4",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:test": "vite build --mode test",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode test",
    "dev:prod": "vite --mode prod",
    "docker:build:dev": "docker-compose build",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:down": "docker-compose down",
    "docker:down:v": "docker-compose down -v",
    "docker:up:dev": "docker-compose up -d",
    "docker:up:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "gen-route": "sa gen-route",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
